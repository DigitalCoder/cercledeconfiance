    public function getId()
    {
        return $this->id;
    }

    /**
     * Set name
     *
     * @param string $file_name
     *
     * @return Cloud
     */
    public function setFileName($fileName)
    {
        $this->fileName = $fileName;

        return $this;
    }

    /**
     * Get file_name
     *
     * @return string
     */
